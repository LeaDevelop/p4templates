{
  "name": "default_unreal_template",
  "depots": [
    {
      "name": "{project}_depot",
      "type": "stream",
      "depth": "1"
    }
  ],
  "groups": [
    {
      "name": "{project}_group",
      "description": "A group I'll be making and deleting often.",
      "max_results": "unset",
      "max_scan_rows": "unset",
      "max_lock_time": "unset",
      "max_open_files": "unset",
      "timeout": "43200",
      "password_timeout": "unset",
      "subgroups": "",
      "owners": [
        "test_dude"
      ],
      "users": [
        "test_dude"
      ]
    }

  ],
  "users": [
    {
      "name": "test_dude",
      "email": "test1@dude.com",
      "full_name": "test dude"
    }
  ],
  "streams": [
    {
      "depot": "{project}_depot",
      "name": "{project}_main",
      "type": "mainline",
      "ignored": [
        "*.pyc",
        "/Intermediate/...",
        "/Saved/...",
        "/DerivedDataCache/...",
        "/Build/..."
      ]
    },
    {
      "depot": "{project}_depot",
      "name": "{project}_dev",
      "type": "development",
      "parent": "{project}_main",
      "ignored": [
        "*.pyc",
        "/Intermediate/...",
        "/Saved/...",
        "/DerivedDataCache/...",
        "/Build/..."
      ]
    }
  ],
  "protections": [
    {
      "access": "write",
      "type": "group",
      "name": "{project}_group",
      "host": "*",
      "path": "//{project}_depot/...",
      "comment": "auto generated"
    }
  ],
  "types": {
    "binary+S2w": [
        "//....app",
        "//....dll",
        "//....dylib",
        "//....exe",
        "//....exp",
        "//....ipa",
        "//....lib",
        "//....pdb",
        "//....stub"
    ],
    "binary+l": [
        "//....uasset",
        "//....ubulk",
        "//....uexp",
        "//....umap",
        "//....upk",
        "//....udk",
        "//....mb",
        "//....psd",
        "//....max",
        "//....blend"
    ],
    "binary+Fl": [
        "//....7z",
        "//....avi",
        "//....bmp",
        "//....bz2",
        "//....gif",
